<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zjw.Mapper.UserMapper">
    <select id="selectUserByAll" resultType="com.zjw.Domain.User">
        select * from user where name = #{username} or phone = #{username} or idcard = #{username}
    </select>
</mapper>